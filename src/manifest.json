{
    "name": "Go Meta labs",
    "version": "0.0",
    "description": "Go Meta wallets extension",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "webNavigation",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js",
            "runtime.js"
        ]
    },
    "browser_action": {
        "default_popup": "index.html"
    }
  }  